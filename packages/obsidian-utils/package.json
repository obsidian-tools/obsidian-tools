{
  "name": "obsidian-utils",
  "version": "0.10.0",
  "description": "A collection of utilities for interacting with or getting information on obsidian plugins",
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "obsidian",
    "obsidian.md",
    "plugin",
    "plugins",
    "lib"
  ],
  "author": "Justin Bennett <zephraph@gmail.com>",
  "homepage": "https://github.com/zephraph/obsidian-tools#readme",
  "license": "MIT",
  "main": "./lib/obsidian-utils.js",
  "module": "./lib/obsidian-utils.esm.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zephraph/obsidian-tools.git"
  },
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "node build",
    "postbuild": "tsc",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown --disableSources --excludePrivate --readme .template/docs-readme.md src/",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --"
  },
  "bugs": {
    "url": "https://github.com/zephraph/obsidian-tools/issues"
  },
  "devDependencies": {
    "@types/date-fns": "2.6.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.14.31",
    "@types/node-fetch": "2.6.2",
    "@types/which": "2.0.0",
    "esbuild": "0.8.57",
    "jest": "27.5.1",
    "obsidian": "obsidianmd/obsidian-api",
    "ts-jest": "27.0.2",
    "typedoc": "0.20.28",
    "typedoc-plugin-markdown": "3.5.0",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "date-fns": "^2.17.0",
    "execa": "^5.0.0",
    "node-fetch": "^2.6.1",
    "which": "^2.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
