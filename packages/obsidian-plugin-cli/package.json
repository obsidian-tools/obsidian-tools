{
  "name": "obsidian-plugin-cli",
  "description": "A CLI tool to make it easy to develop plugins for obisidian",
  "version": "0.9.0",
  "author": "zephraph <zephraph@gmail.com>",
  "bin": {
    "obsidian-plugin": "./bin/run"
  },
  "bugs": "https://github.com/zephraph/obsidian-tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@zephraph/procedure": "^0.2.0",
    "ansi-colors": "^4.1.1",
    "cosmiconfig": "^7.0.0",
    "dedent": "^0.7.0",
    "esbuild": "^0.24.0",
    "obsidian-utils": "^0.10.2",
    "prompts": "^2.4.0",
    "tslib": "^1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@types/dedent": "0.7.0",
    "@types/node": "10.17.60",
    "@types/prompts": "2.4.2",
    "@types/update-notifier": "5.1.0",
    "builtin-modules": "^3.3.0",
    "globby": "10.0.2",
    "ts-node": "8.10.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zephraph/obsidian-tools",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "obsidian-plugin",
    "hooks": {
      "init": "./lib/hooks/update-notifier"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/zephraph/obsidian-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
