{
  "name": "obsidian-tools",
  "version": "1.0.0",
  "description": "A collection of utilities for obsidian",
  "private": true,
  "repository": "https://github.com/zephraph/obsidian-tools",
  "author": "zephraph <zephraph@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "lerna run build",
    "release": "auto shipit"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/esbuild"
    ]
  },
  "devDependencies": {
    "@auto-it/all-contributors": "10.16.7",
    "@auto-it/first-time-contributor": "10.32.6",
    "auto": "10.46.0",
    "lerna": "3.22.1"
  },
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor",
      "released"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
